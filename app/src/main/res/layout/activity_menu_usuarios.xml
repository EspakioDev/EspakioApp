<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_espakio"
    android:padding="10dp"
    tools:context="com.development.espakio.appespakio.activities.MenuUsuarios">

    <ImageView
        android:id="@+id/menuUsu_imgUsuarioUno"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="70dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/menuUsu_imgUsuarioTres"
        app:layout_constraintEnd_toStartOf="@+id/menuUsu_imgUsuarioDos"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/img_usuario_1" />

    <TextView
        android:id="@+id/menuUsu_txtUsernameUno"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/menuUsu_imgUsuarioUno"
        android:layout_below="@+id/menuUsu_imgUsuarioUno"
        android:text="UserName"
        android:textAlignment="center"
        android:textColor="@color/Blanco"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/menuUsu_imgUsuarioDos"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignEnd="@+id/menuUsu_imgUsuarioPlus"
        android:layout_alignTop="@+id/menuUsu_imgUsuarioUno"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/menuUsu_imgUsuarioPlus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_usuario_1" />

    <TextView
        android:id="@+id/menuUsu_txtUsernameDos"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/menuUsu_imgUsuarioDos"
        android:layout_below="@+id/menuUsu_imgUsuarioUno"
        android:text="UserName"
        android:textAlignment="center"
        android:textColor="@color/Blanco"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/menuUsu_imgUsuarioTres"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/menuUsu_imgUsuarioUno"
        android:layout_marginBottom="139dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_usuario_1" />

    <TextView
        android:id="@+id/menuUsu_txtUsernameTres"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/menuUsu_imgUsuarioTres"
        android:layout_below="@+id/menuUsu_imgUsuarioTres"
        android:layout_marginTop="-139dp"
        android:text="UserName"
        android:textAlignment="center"
        android:textColor="@color/Blanco"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/menuUsu_imgUsuarioPlus"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:padding="10dp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/menuUsu_imgUsuarioTres"
        app:srcCompat="@drawable/img_usuarioplus"
        tools:layout_editor_absoluteY="256dp" />

    <Button
        android:id="@+id/menuUsu_btnConfig"
        android:layout_width="83dp"
        android:layout_height="83dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/icon_config_espakio"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toStartOf="parent" />


</RelativeLayout>
